---
import BlogListPage from '../../../components/pages/BlogListPage.astro';
import { collectPosts } from '../../../utils/collectPosts';
import { getNavLinks } from '../../../i18n/ui';

const posts = await collectPosts('en')

const t = {
	lang: 'en' as const,
	readText: 'Read',
	articles: 'Articles',
	notes: 'Notes',
	jots: 'Jots',
	navLinks: getNavLinks('en'),
};
---

<BlogListPage {posts} {t} />
